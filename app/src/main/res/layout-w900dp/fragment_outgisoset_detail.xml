        <com.sap.cloud.mobile.fiori.misc.KeyValueCell
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            app:keyText="Delivery"
            app:valueText="@{outGiSo.getOptionalValue(OutGiSo.delivery).toString()}" />

        <com.sap.cloud.mobile.fiori.misc.KeyValueCell
